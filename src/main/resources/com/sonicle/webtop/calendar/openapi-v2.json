          {
            "name": "hrefs",
            "in": "query",
            "style": "form",
            "explode": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/DavCalObject"
                  }
                }
              }
            }
          }
        },
        "description": "List all CalendarObjects of specified Calendar."
      },
      "post": {
        "tags": [
          "dav"
        ],
        "summary": "Adds a new calendar object",
        "operationId": "addCalObject",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DavCalObjectNew"
