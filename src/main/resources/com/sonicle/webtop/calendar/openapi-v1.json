{
  "swagger": "2.0",
  "info": {
    "version": "v1",
    "title": "WebTop Calendar",
    "contact": {
      "email": "dev-team@sonicle.com"
    }
  },
  "security": [
    {
      "Basic authentication": []
    }
  ],
  "paths": {
    "/caldav/calendars": {
      "get": {
        "tags": [
          "dav-calendars"
        ],
        "summary": "List all calendars",
        "operationId": "getCalendars",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Calendar"
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "dav-calendars"
        ],
        "summary": "Adds a new calendar",
        "operationId": "addCalendar",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/CalendarNew"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/Calendar"
            }
          }
        }
      }
    },
    "/caldav/calendars/{calendarUid}": {
      "get": {
        "tags": [
          "dav-calendars"
        ],
        "summary": "Gets a single calendar",
        "operationId": "getCalendar",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/Calendar"
            }
          },
          "400": {
            "description": "Invalid parameter"
          },
          "404": {
            "description": "Calendar not found"
          }
        }
      },
      "put": {
        "tags": [
          "dav-calendars"
        ],
        "summary": "Updates a calendar",
        "operationId": "updateCalendar",
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/CalendarUpdate"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          },
          "400": {
            "description": "Invalid parameter"
          },
          "404": {
            "description": "Calendar not found"
          }
        }
      },
      "delete": {
        "tags": [
          "dav-calendars"
        ],
        "summary": "Deletes a calendar",
        "operationId": "deleteCalendar",
        "parameters": [],
        "responses": {
          "204": {
            "description": "Success"
          },
          "400": {
            "description": "Invalid parameter"
          },
          "404": {
            "description": "Calendar not found"
          },
          "405": {
            "description": "Delete operation is not allowed"
          }
        }
      },
      "parameters": [
        {
          "name": "calendarUid",
          "in": "path",
          "required": true,
          "type": "string",
          "description": "Calendar UID",
          "x-example": "1234"
        }
      ]
    },
    "/caldav/calendars/{calendarUid}/objects": {
      "get": {
        "tags": [
          "dav-cal-objects"
        ],
        "summary": "List all calendar objects belonging to a specific calendar",
        "operationId": "getCalObjects",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "hrefs",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/CalObject"
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "dav-cal-objects"
        ],
        "summary": "Adds a new calendar object",
        "operationId": "addCalObject",
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/CalObjectNew"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Success"
          }
        }
      },
      "parameters": [
        {
          "name": "calendarUid",
          "in": "path",
          "required": true,
          "type": "string",
          "description": "Calendar UID",
          "x-example": "1234"
        }
      ]
    },
    "/caldav/calendars/{calendarUid}/objects/changes": {
      "get": {
        "tags": [
          "dav-cal-objects"
        ],
        "summary": "Get calendar object changes",
        "description": "Returns changed calendar objects (added/modified/deleted) since the specified syncToken. If token is not provided, the initial sync configuration will be returned.",
        "operationId": "getCalObjectsChanges",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "syncToken",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Marks changes starting point",
            "x-example": "20180416130954"
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "type": "integer",
            "description": "Limits the number of returned results",
            "x-example": 100
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/CalObjectsChanges"
            }
          },
          "400": {
            "description": "Invalid parameter"
          }
        }
      },
      "parameters": [
        {
          "name": "calendarUid",
          "in": "path",
          "required": true,
          "type": "string",
          "description": "Calendar UID",
          "x-example": "1234"
        }
      ]
    },
    "/caldav/calendars/{calendarUid}/objects/{href}": {
      "get": {
        "tags": [
          "dav-cal-objects"
        ],
        "summary": "Get a single calendar object",
        "operationId": "getCalObject",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/CalObject"
            }
          },
          "400": {
            "description": "Invalid parameter"
          },
          "404": {
            "description": "Object not found"
          }
        }
      },
      "put": {
        "tags": [
          "dav-cal-objects"
        ],
        "summary": "Updates a calendar object",
        "operationId": "updateCalObject",
        "consumes": [
          "text/calendar"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          },
          "400": {
            "description": "Invalid parameter"
          },
          "404": {
            "description": "Object not found"
          }
        }
      },
      "delete": {
        "tags": [
          "dav-cal-objects"
        ],
        "summary": "Deletes a calendar object",
        "operationId": "deleteCalObject",
        "parameters": [],
        "responses": {
          "204": {
            "description": "Success"
          },
          "400": {
            "description": "Invalid parameter"
          },
          "404": {
            "description": "Object not found"
          }
        }
      },
      "parameters": [
        {
          "name": "calendarUid",
          "in": "path",
          "required": true,
          "type": "string",
          "description": "Calendar UID",
          "x-example": "1234"
        },
        {
          "name": "href",
          "in": "path",
          "required": true,
          "type": "string",
          "description": "CalObject reference URI"
        }
      ]
    },
    "/eas/folders": {
      "get": {
        "tags": [
          "eas-folders"
        ],
        "summary": "List all folders",
        "operationId": "getFolders",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/SyncFolder"
              }
            }
          }
        }
      }
    },
    "/eas/folders/{folderId}/messages-stats": {
      "get": {
        "tags": [
          "eas-messages"
        ],
        "summary": "List all messages for a specific folder",
        "operationId": "getMessagesStats",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "cutoffDate",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "Cut-off date (ISO date/time YYYYMMDD'T'HHMMSS'Z')",
            "x-example": "20150912T000000Z"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/SyncEventStat"
              }
            }
          },
          "400": {
            "description": "Invalid parameter"
          }
        }
      },
      "parameters": [
        {
          "name": "folderId",
          "in": "path",
          "required": true,
          "type": "integer",
          "description": "Folder ID",
          "x-example": 1234
        }
      ]
    },
    "/eas/folders/{folderId}/messages": {
      "post": {
        "tags": [
          "eas-messages"
        ],
        "summary": "Adds a message",
        "operationId": "addMessage",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/SyncEventUpdate"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/SyncEventStat"
            }
          }
        }
      },
      "parameters": [
        {
          "name": "folderId",
          "in": "path",
          "required": true,
          "type": "integer",
          "description": "Folder ID",
          "x-example": 1234
        }
      ]
    },
    "/eas/folders/{folderId}/messages/{id}": {
      "get": {
        "tags": [
          "eas-messages"
        ],
        "summary": "Get a single message",
        "operationId": "getMessage",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/SyncEvent"
            }
          },
          "400": {
            "description": "Invalid parameter"
          },
          "404": {
            "description": "Message not found"
          }
        }
      },
      "put": {
        "tags": [
          "eas-messages"
        ],
        "summary": "Updates a message",
        "operationId": "updateMessage",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/SyncEventUpdate"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/SyncEventStat"
              }
            }
          },
          "400": {
            "description": "Invalid parameter"
          },
          "404": {
            "description": "Message not found"
          }
        }
      },
      "delete": {
        "tags": [
          "eas-messages"
        ],
        "summary": "Deletes a message",
        "operationId": "deleteMessage",
        "parameters": [],
        "responses": {
          "204": {
            "description": "Success"
          },
          "400": {
            "description": "Invalid parameter"
          },
          "404": {
            "description": "Message not found"
          }
        }
      },
      "parameters": [
        {
          "name": "folderId",
          "in": "path",
          "required": true,
          "type": "integer",
          "description": "Folder ID",
          "x-example": 1234
        },
        {
          "name": "id",
          "in": "path",
          "required": true,
          "type": "integer",
          "description": "Message ID",
          "x-example": 1234
        }
      ]
    }
  },
  "securityDefinitions": {
    "Basic authentication": {
      "type": "basic"
    }
  },
  "definitions": {
    "ApiError": {
      "type": "object",
      "required": [
        "code"
      ],
      "properties": {
        "code": {
          "type": "integer",
          "minimum": 400,
          "maximum": 599
        },
        "description": {
          "type": "string",
          "example": "Bad query parameter [$size]: Invalid integer value [abc]"
        }
      },
      "description": "This general detailed error structure is used throughout this API.",
      "example": "{\n  \"code\": 400,\n  \"description\": \"Bad query parameter [$size]: Invalid integer value [abc]\"\n}"
    },
    "Calendar": {
      "type": "object",
      "required": [
        "aclEle",
        "aclFol",
        "displayName",
        "id",
        "ownerUsername",
        "syncToken",
        "uid"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "description": "Calendar ID (internal)"
        },
        "uid": {
          "type": "string",
          "description": "Calendar UID (public)"
        },
        "displayName": {
          "type": "string",
          "description": "Display name"
        },
        "description": {
          "type": "string",
          "description": "Description"
        },
        "color": {
          "type": "string",
          "description": "Assigned color"
        },
        "syncToken": {
          "type": "string",
          "description": "Current sync token"
        },
        "aclFol": {
          "type": "string",
          "description": "ACL info for folder itself"
        },
        "aclEle": {
          "type": "string",
          "description": "ACL info for folder elements"
        },
        "ownerUsername": {
          "type": "string",
          "description": "The owner profile's username"
        }
      },
      "description": "Bean for carry calendar's fields"
    },
    "CalendarNew": {
      "type": "object",
      "required": [
        "displayName"
      ],
      "properties": {
        "displayName": {
          "type": "string",
          "description": "Display name"
        },
        "description": {
          "type": "string",
          "description": "Description"
        },
        "color": {
          "type": "string",
          "description": "Color"
        }
      },
      "description": "Bean for carry addressbook's fields"
    },
    "CalendarUpdate": {
      "type": "object",
      "properties": {
        "displayName": {
          "type": "string",
          "description": "New value for displayName"
        },
        "description": {
          "type": "string",
          "description": "New value for description"
        },
        "color": {
          "type": "string",
          "description": "New value for color"
        },
        "updatedFields": {
          "type": "array",
          "description": "Specifies which of above fields have been updated",
          "items": {
            "type": "string"
          }
        }
      },
      "description": "Bean for carry calendar's updateable fields"
    },
    "CalObject": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "description": "CalObject ID (internal)"
        },
        "uid": {
          "type": "string",
          "description": "CalObject UID (public)"
        },
        "href": {
          "type": "string",
          "description": "Reference URI"
        },
        "lastModified": {
          "type": "integer",
          "format": "int64",
          "description": "Last modification time (unix timestamp)"
        },
        "etag": {
          "type": "string",
          "description": "Revision tag"
        },
        "size": {
          "type": "integer",
          "description": "Size (in bytes) of calendar data"
        },
        "icalendar": {
          "type": "string",
          "description": "Calendar data (iCalendar format)"
        }
      },
      "description": "Bean for carry calendar object's fields"
    },
    "CalObjectNew": {
      "type": "object",
      "required": [
        "href"
      ],
      "properties": {
        "href": {
          "type": "string",
          "description": "Reference URI"
        },
        "icalendar": {
          "type": "string",
          "description": "Calendar data (iCalendar format)"
        }
      },
      "description": "Bean for carry new calendar object's fields"
    },
    "CalObjectChanged": {
      "type": "object",
      "required": [
        "etag",
        "href",
        "id"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "description": "CalObject ID (internal)"
        },
        "href": {
          "type": "string",
          "description": "Reference URI"
        },
        "etag": {
          "type": "string",
          "description": "Revision tag"
        }
      },
      "description": "Bean for carry fields of calendar object that have been changed"
    },
    "CalObjectsChanges": {
      "type": "object",
      "required": [
        "deleted",
        "inserted",
        "syncToken",
        "updated"
      ],
      "properties": {
        "syncToken": {
          "type": "string",
          "description": "Current sync token"
        },
        "inserted": {
          "type": "array",
          "description": "Items that have been inserted",
          "items": {
            "$ref": "#/definitions/CalObjectChanged"
          }
        },
        "updated": {
          "type": "array",
          "description": "Items that have been updated",
          "items": {
            "$ref": "#/definitions/CalObjectChanged"
          }
        },
        "deleted": {
          "type": "array",
          "description": "Items that have been deleted",
          "items": {
            "$ref": "#/definitions/CalObjectChanged"
          }
        }
      },
      "description": "Bean for carry calendar object collection changes"
    },
    "SyncFolder": {
      "type": "object",
      "required": [
        "deflt",
        "displayName",
        "elAcl",
        "etag",
        "foAcl",
        "id",
        "ownerId"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "description": "Calendar ID (internal)"
        },
        "displayName": {
          "type": "string",
          "description": "Display name"
        },
        "etag": {
          "type": "string",
          "description": "Revision tag"
        },
        "deflt": {
          "type": "boolean",
          "description": "Specifies if marked as predefined folder"
        },
        "foAcl": {
          "type": "string",
          "description": "ACL info for folder itself"
        },
        "elAcl": {
          "type": "string",
          "description": "ACL info for folder elements"
        },
        "ownerId": {
          "type": "string",
          "description": "The owner profile ID"
        }
      },
      "description": "Bean for carry calendarâ€™s fields"
    },
    "SyncEventStat": {
      "type": "object",
      "required": [
        "etag",
        "id"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "description": "Event ID (internal)"
        },
        "etag": {
          "type": "string",
          "description": "Revision tag"
        }
      },
      "description": "Bean for carry event's stat fields"
    },
    "SyncEvent": {
      "type": "object",
      "required": [
        "allDay",
        "attendees",
        "busy",
        "end",
        "etag",
        "id",
        "organizer",
        "prvt",
        "start",
        "tz"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "description": "Event ID (internal)"
        },
        "etag": {
          "type": "string",
          "description": "Revision tag"
        },
        "start": {
          "type": "string",
          "description": "Start date/time (ISO format)"
        },
        "end": {
          "type": "string",
          "description": "End date/time (ISO format)"
        },
        "tz": {
          "type": "string",
          "description": "Timezone ID"
        },
        "allDay": {
          "type": "boolean",
          "description": "All day flag"
        },
        "organizer": {
          "type": "string",
          "description": "Organizer address (as RFC822)"
        },
        "title": {
          "type": "string",
          "description": "Title"
        },
        "description": {
          "type": "string",
          "description": "Description"
        },
        "location": {
          "type": "string",
          "description": "Location"
        },
        "prvt": {
          "type": "boolean",
          "description": "Private flag"
        },
        "busy": {
          "type": "boolean",
          "description": "Busy flag"
        },
        "reminder": {
          "type": "integer",
          "description": "Reminder"
        },
        "recRule": {
          "type": "string",
          "description": "Recurrence RULE string"
        },
        "recStart": {
          "type": "string",
          "description": "Recurrence start date (ISO date YYYYMMDD)"
        },
        "exDates": {
          "type": "array",
          "description": "Excluded dates (ISO date YYYYMMDD)",
          "items": {
            "type": "string"
          }
        },
        "attendees": {
          "type": "array",
          "description": "Appointment attendees",
          "items": {
            "$ref": "#/definitions/SyncEventDataAttendee"
          }
        }
      },
      "description": "Bean for carry event's fields"
    },
    "SyncEventUpdate": {
      "type": "object",
      "required": [
        "data"
      ],
      "properties": {
        "data": {
          "$ref": "#/definitions/SyncEventData"
        },
        "exceptions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/SyncEventData"
          }
        }
      },
      "description": "Bean for carry event's updateable fields"
    },
    "SyncEventData": {
      "type": "object",
      "required": [
        "allDay",
        "attendees",
        "busy",
        "end",
        "organizer",
        "prvt",
        "start",
        "tz"
      ],
      "properties": {
        "start": {
          "type": "string",
          "description": "Start date/time (ISO date/time YYYYMMDD'T'HHMMSS'Z')"
        },
        "end": {
          "type": "string",
          "description": "End date/time (ISO date/time YYYYMMDD'T'HHMMSS'Z')"
        },
        "tz": {
          "type": "string",
          "description": "Timezone ID"
        },
        "allDay": {
          "type": "boolean",
          "description": "All day flag"
        },
        "organizer": {
          "type": "string",
          "description": "Organizer address (as RFC822)"
        },
        "title": {
          "type": "string",
          "description": "Title"
        },
        "description": {
          "type": "string",
          "description": "Description"
        },
        "location": {
          "type": "string",
          "description": "Location"
        },
        "prvt": {
          "type": "boolean",
          "description": "Private flag"
        },
        "busy": {
          "type": "boolean",
          "description": "Busy flag"
        },
        "reminder": {
          "type": "integer",
          "description": "Reminder"
        },
